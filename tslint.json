{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
        "array-type": [true, "array"],
        "member-access": false,
        "object-literal-sort-keys": false,
        "ban-types": false,
        "radix": false
    },
    "rulesDirectory": []
}