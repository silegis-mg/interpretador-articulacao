          Inciso {
            "alineas": [],
            "derivacoes": [
              "alineas",
            ],
            "descricao": "negativa, quando em nome da pessoa sobre a qual se pede a informação não houver créditos tributários.",
            "numero": "II",
            "tipo": "inciso",
          },
        ],
        "numero": "219",
        "paragrafos": [],
        "tipo": "artigo",
      },
