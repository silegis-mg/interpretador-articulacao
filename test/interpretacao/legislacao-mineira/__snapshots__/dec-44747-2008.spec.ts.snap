            "tipo": "inciso",
          },
          Inciso {
            "alineas": [],
            "derivacoes": [
              "alineas",
            ],
            "descricao": "transação de qualquer natureza com órgãos públicos ou autárquicos estaduais;",
            "numero": "II",
            "tipo": "inciso",
          },
          Inciso {
            "alineas": [],
            "derivacoes": [
