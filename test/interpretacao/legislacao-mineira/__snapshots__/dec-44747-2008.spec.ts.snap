          "subsecoes",
        ],
        "descricao": "Do Pedido de Retificação
(Seção acrescentada pelo art. 42 do Decreto nº 47.725, de 1º/10/2019.)",
        "numero": "IX-A",
        "subsecoes": [],
        "tipo": "secao",
      },
      Secao {
        "artigos": [
          Artigo {
            "derivacoes": [
              "incisos",
              "paragrafos",
            ],
            "descricao": "A representação fiscal para fins penais relativa aos crimes contra a ordem tributária definidos nos arts. 1º e 2º da Lei Federal nº 8.137, de 27 de dezembro de 1990, será encaminhada ao Ministério Público para o procedimento criminal cabível, após proferida decisão final na esfera administrativa sobre a exigência fiscal do crédito tributário correspondente.",
            "incisos": [],
            "numero": "171",
            "paragrafos": [],
            "tipo": "artigo",
          },
        ],
        "derivacoes": [
          "artigos",
          "subsecoes",
        ],
        "descricao": "Da Representação Fiscal Para Fins Penais",
        "numero": "X",
        "subsecoes": [],
