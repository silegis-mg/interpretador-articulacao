            "tipo": "inciso",
          },
          Inciso {
            "alineas": [],
            "derivacoes": [
              "alineas",
            ],
            "descricao": "a inscrição como contribuinte, a alteração cadastral que envolva inclusão ou substituição de sócio e a reativação da inscrição estadual;",
            "numero": "III",
            "tipo": "inciso",
          },
          Inciso {
            "alineas": [],
            "derivacoes": [
              "alineas",
            ],
            "descricao": "baixa de inscrição como contribuinte;",
            "numero": "IV",
            "tipo": "inciso",
          },
          Inciso {
