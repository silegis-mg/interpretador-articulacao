        "numero": "28",
        "paragrafos": [
          Paragrafo {
            "derivacoes": [
              "incisos",
            ],
            "descricao": "Para os efeitos do disposto neste artigo, o interessado instruirá o requerimento com:",
            "incisos": [
              Inciso {
                "alineas": [],
                "derivacoes": [
                  "alineas",
                ],
                "descricao": "cópia do comprovante do recolhimento indevido, se for o caso;",
