                        ],
                        "descricao": "a data e a hora em que se processou a votação;",
                        "numero": "I",
                        "tipo": "inciso",
                      },
                      Inciso {
                        "alineas": [],
                        "derivacoes": [
                          "alineas",
                        ],
                        "descricao": "a matéria objeto da votação;",
                        "numero": "II",
                        "tipo": "inciso",
                      },
                      Inciso {
