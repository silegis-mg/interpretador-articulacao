            "numero": "155",
            "paragrafos": [
              Paragrafo {
                "derivacoes": [
                  "incisos",
                ],
                "descricao": "Não será autorizada a publicação de pronunciamento que contiver violação a direito constitucional ou a norma regimental.",
                "incisos": [],
                "numero": "1",
                "tipo": "paragrafo",
              },
              Paragrafo {
                "derivacoes": [
                  "incisos",
                ],
                "descricao": "Poderão o orador e o aparteante rever o seu pronunciamento, em prazo não superior a vinte e quatro horas.",
