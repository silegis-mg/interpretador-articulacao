              },
              Inciso {
                "alineas": [],
                "derivacoes": [
                  "alineas",
                ],
                "descricao": "dar explicação pessoal;",
                "numero": "IV",
                "tipo": "inciso",
              },
              Inciso {
                "alineas": [],
                "derivacoes": [
                  "alineas",
                ],
                "descricao": "fazer comunicação;",
                "numero": "V",
                "tipo": "inciso",
              },
              Inciso {
                "alineas": [],
                "derivacoes": [
                  "alineas",
                ],
                "descricao": "falar sobre assunto de interesse público;",
