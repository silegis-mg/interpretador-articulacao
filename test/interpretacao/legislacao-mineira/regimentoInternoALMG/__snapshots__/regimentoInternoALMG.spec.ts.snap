            ],
            "tipo": "artigo",
          },
          Artigo {
            "derivacoes": [
              "incisos",
              "paragrafos",
            ],
            "descricao": "Havendo descumprimento deste regimento no curso dos debates, o Presidente da Assembleia adotará qualquer das seguintes providências:",
            "incisos": [
              Inciso {
                "alineas": [],
                "derivacoes": [
