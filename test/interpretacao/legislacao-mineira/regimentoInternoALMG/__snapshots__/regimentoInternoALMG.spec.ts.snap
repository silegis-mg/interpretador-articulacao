                "derivacoes": [
                  "incisos",
                  "paragrafos",
                ],
                "descricao": "Na tramitação sob regime de urgência, serão observadas as exigências regimentais, com as seguintes ressalvas:",
                "incisos": [
                  Inciso {
                    "alineas": [],
                    "derivacoes": [
                      "alineas",
                    ],
                    "descricao": "dispensa da exigência de prévia publicação dos pareceres e demais proposições acessórias;",
