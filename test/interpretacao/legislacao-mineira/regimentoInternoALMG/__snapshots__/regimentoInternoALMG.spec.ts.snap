            "incisos": [],
            "numero": "129",
            "paragrafos": [
              Paragrafo {
                "derivacoes": [
                  "incisos",
                ],
                "descricao": "Na ausência dos Presidentes, caberá a direção dos trabalhos a um dos Vice-Presidentes, observada a ordem decrescente de idade, ou, na falta destes, ao mais idoso dos membros presentes.",
                "incisos": [],
                "numero": "1",
                "tipo": "paragrafo",
              },
              Paragrafo {
                "derivacoes": [
                  "incisos",
                ],
                "descricao": "Quando a Mesa da Assembleia participar da reunião, os trabalhos serão dirigidos pelo seu Presidente.",
